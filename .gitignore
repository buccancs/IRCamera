# ===================================================================
# Recommended .gitignore for Android Projects
# ===================================================================

# IDE - Android Studio
# -------------------------------------------------------------------
# User-specific files
.idea/
*.aar
*.xml
*.so
*.jar

# Generated files
*.iml
.cxx/
.externalNativeBuild/
/captures/


# Build
# -------------------------------------------------------------------
/build/
*/build/
**/build/
*.apk
*.aab
*.ap_
*.dex
*.class
*.hprof
*.snapshot
lint-results-*
lint-results*.html
lint-results*.xml
/build-cache/
**/build-cache/
*.war
*.jar
.navigation/
captures/
.externalNativeBuild/
.cxx/
obj/


# Gradle
# -------------------------------------------------------------------
.gradle/
.build-cache/
buildSrc/build/
buildSrc/.gradle/
gradle-app.setting
.gradletasknamecache
gradle/wrapper/gradle-wrapper.jar


# Local configuration
# -------------------------------------------------------------------
# Contains SDK path, API keys, and other user-specific information.
local.properties


# Signing files
# -------------------------------------------------------------------
# Keep these files private and out of version control.
*.jks
*.keystore


# OS-generated files
# -------------------------------------------------------------------
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
Thumbs.db
ehthumbs.db
desktop.ini
$RECYCLE.BIN/
*.tmp
*.temp

# IDE and cache files
# -------------------------------------------------------------------
# IntelliJ IDEA
.idea/shelf/
.idea/workspace.xml
.idea/tasks.xml
.idea/dictionaries/
.idea/vcs.xml
.idea/jsLibraryMappings.xml
.idea/datasources.xml
.idea/datasources/
.idea/sqlDataSources.xml
.idea/dynamic.xml
.idea/uiDesigner.xml
.idea/libraries/
.idea/gradle.xml
.idea/misc.xml
.idea/modules.xml
.idea/navEditor.xml
.idea/assetWizardSettings.xml
.idea/deploymentTargetDropDown.xml
.idea/inspectionProfiles/
.idea/sonarlint/

# Android Studio cache
.android/
android.tests.runner/
.metadata/
.recommenders/
.gradle-wrapper/


# App/Project specific directories
# -------------------------------------------------------------------
# These are your custom rules, which are perfectly fine to keep.
/app/prod/
/app/prodTopdon/
/ai-upscale/
/ai-upscale/3rdparty/ncnn-android-vulkan-shared/armeabi-v7a/

