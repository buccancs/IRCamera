# ===================================================================
# Recommended .gitignore for Android Projects
# ===================================================================

# IDE - Android Studio
# -------------------------------------------------------------------
# User-specific files
.idea/

# Generated files
*.iml
.cxx/
.externalNativeBuild/
/captures/


# Build
# -------------------------------------------------------------------
/build/
*/build/
**/build/
*.apk
*.aab
*.ap_
*.dex
*.class
*.hprof
*.snapshot
lint-results-*
lint-results*.html
lint-results*.xml
/build-cache/
**/build-cache/
*.war
*.jar
.navigation/
captures/
.externalNativeBuild/
.cxx/
obj/

# Additional Build Artifacts and Generated Files
# -------------------------------------------------------------------
# Generated AAR files in build outputs
build/outputs/**/*.aar
*/build/outputs/**/*.aar
**/build/outputs/**/*.aar

# Generated Java files in build directories
build/generated/**/*.java
*/build/generated/**/*.java
**/build/generated/**/*.java

# Generated resource directories in build
build/generated/**/res/
*/build/generated/**/res/
**/build/generated/**/res/
build/intermediates/**/res/
*/build/intermediates/**/res/
**/build/intermediates/**/res/

# Generated layout directories in build
build/generated/**/layout/
*/build/generated/**/layout/
**/build/generated/**/layout/
build/intermediates/**/layout/
*/build/intermediates/**/layout/
**/build/intermediates/**/layout/

# Build output APKs and AABs
build/outputs/apk/
*/build/outputs/apk/
**/build/outputs/apk/
build/outputs/bundle/
*/build/outputs/bundle/
**/build/outputs/bundle/

# Generated native libraries in build
build/intermediates/**/*.so
*/build/intermediates/**/*.so
**/build/intermediates/**/*.so
build/outputs/**/*.so
*/build/outputs/**/*.so
**/build/outputs/**/*.so


# Gradle
# -------------------------------------------------------------------
.gradle/
.build-cache/
buildSrc/build/
buildSrc/.gradle/
gradle-app.setting
.gradletasknamecache
gradle/wrapper/gradle-wrapper.jar

# Build artifacts and compiler cache that should not be committed
.kotlin/
*.log
build_*.log


# Local configuration
# -------------------------------------------------------------------
# Contains SDK path, API keys, and other user-specific information.
local.properties


# Signing files
# -------------------------------------------------------------------
# Keep these files private and out of version control.
*.jks
*.keystore


# OS-generated files
# -------------------------------------------------------------------
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
Thumbs.db
ehthumbs.db
desktop.ini
$RECYCLE.BIN/
*.tmp
*.temp

# IDE and cache files
# -------------------------------------------------------------------
# IntelliJ IDEA
.idea/shelf/
.idea/workspace.xml
.idea/tasks.xml
.idea/dictionaries/
.idea/vcs.xml
.idea/jsLibraryMappings.xml
.idea/datasources.xml
.idea/datasources/
.idea/sqlDataSources.xml
.idea/dynamic.xml
.idea/uiDesigner.xml
.idea/libraries/
.idea/gradle.xml
.idea/misc.xml
.idea/modules.xml
.idea/navEditor.xml
