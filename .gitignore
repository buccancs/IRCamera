# ===================================================================
# Recommended .gitignore for Android Projects
# ===================================================================

# IDE - Android Studio
# -------------------------------------------------------------------
# User-specific files
.idea/caches/
.idea/libraries/
.idea/modules.xml
.idea/workspace.xml
.idea/navEditor.xml
.idea/assetWizardSettings.xml

# Generated files
*.iml
.cxx/
.externalNativeBuild/
/captures/


# Build
# -------------------------------------------------------------------
/build/
*/build/
**/build/
*.apk
*.aab
*.ap_
*.dex
*.class
*.hprof
*.snapshot
lint-results-*


# Gradle
# -------------------------------------------------------------------
.gradle/
.build-cache/
buildSrc/build/
buildSrc/.gradle/


# Local configuration
# -------------------------------------------------------------------
# Contains SDK path, API keys, and other user-specific information.
local.properties


# Signing files
# -------------------------------------------------------------------
# Keep these files private and out of version control.
*.jks
*.keystore


# OS-generated files
# -------------------------------------------------------------------
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
Thumbs.db
ehthumbs.db


# App/Project specific directories
# -------------------------------------------------------------------
# These are your custom rules, which are perfectly fine to keep.
/app/prod/
/app/prodTopdon/
/ai-upscale/
/ai-upscale/3rdparty/ncnn-android-vulkan-shared/armeabi-v7a/
